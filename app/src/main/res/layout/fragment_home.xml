<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#141414"
    android:orientation="vertical"
    app:ignore="NamespaceTypo">


    <include layout="@layout/layout_search" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#383838">

        <itbour.onetouchshow.custom.DesignTableLayout
            android:background="#141414"
            android:id="@+id/id_recyview"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:layout_gravity="center_vertical"
            android:scrollbars="none"
            app:rtl_tabIndicatorColor="@color/idselcte"
            app:rtl_tabMaxWidth=" 264dp"
            app:rtl_tabMinWidth="120dip" />

    </LinearLayout>
    <View
        android:id="@+id/home_bar_padding"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@color/apptheme"/>
    <com.ys.uilibrary.vp.NoScrollViewPager
        android:id="@+id/id_vp_home"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/apptheme"
        android:paddingTop="5dp" />

</LinearLayout>
